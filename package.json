{
  "name": "reactServerless",
  "version": "1.0.0",
  "description": "Serverless webpack example using ecma script",
  "scripts": {
    "test": "./node_modules/.bin/mocha --require babel-register",
    "test:coverage": "babel-node ./node_modules/.bin/isparta cover _mocha",
    "server": "sls offline start",
    "client": "cd client/site && yarn start",
    "dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\"",
    "build:site": "cd client/site && yarn build && cd ../../ && serverless client deploy",
    "build:account": "serverless client deploy --bucketName account-sher.rapidstax.com --distributionFolder client/account/build",
    "build:admin": "serverless client deploy --bucketName admin-sher.rapidstax.com --distributionFolder client/admin/build",
    "remove:site": "serverless client remove",
    "remove:account": "serverless client remove --bucketName account-sher.rapidstax.com",
    "remove:admin": "serverless client remove --bucketName admin-sher.rapidstax.com"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^4.1.2",
    "concurrently": "^3.5.1",
    "isparta": "^4.0.0",
    "istanbul": "^0.4.5",
    "mocha": "^5.0.1",
    "serverless-finch": "^1.4.0",
    "serverless-offline": "^3.17.0",
    "serverless-webpack": "^3.1.1",
    "webpack": "^3.3.0"
  },
  "author": "mosufy <mosufy@gmail.com>",
  "license": "MIT"
}
