{
  "name": "reactServerless",
  "version": "1.0.0",
  "description": "Serverless webpack example using ecma script",
  "scripts": {
    "test": "./node_modules/.bin/mocha --require babel-register",
    "test:coverage": "babel-node ./node_modules/.bin/isparta cover _mocha",
    "server": "sls offline start",
    "client": "cd client && yarn start",
    "dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\""
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^4.1.2",
    "concurrently": "^3.5.1",
    "isparta": "^4.0.0",
    "istanbul": "^0.4.5",
    "mocha": "^5.0.1",
    "serverless-offline": "^3.17.0",
    "serverless-webpack": "^3.1.1",
    "webpack": "^3.3.0"
  },
  "author": "mosufy <mosufy@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "lodash": "^4.17.5",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0"
  }
}
